{
  "dashboard": {
    "id": null,
    "title": "Premier League MLOps - Model Performance & Drift Monitoring",
    "tags": ["mlops", "evidently", "premier-league"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Model Performance Overview",
        "type": "stat",
        "targets": [
          {
            "expr": "avg(model_accuracy)",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {
                  "color": "red",
                  "value": 0
                },
                {
                  "color": "yellow",
                  "value": 0.55
                },
                {
                  "color": "green",
                  "value": 0.65
                }
              ]
            },
            "unit": "percentunit"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 0,
          "y": 0
        }
      },
      {
        "id": 2,
        "title": "Drift Detection Status",
        "type": "stat",
        "targets": [
          {
            "expr": "drift_detected",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": 0
                },
                {
                  "color": "red",
                  "value": 1
                }
              ]
            },
            "mappings": [
              {
                "options": {
                  "0": {
                    "text": "No Drift"
                  },
                  "1": {
                    "text": "Drift Detected"
                  }
                },
                "type": "value"
              }
            ]
          }
        },
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 6,
          "y": 0
        }
      },
      {
        "id": 3,
        "title": "Predictions Generated (24h)",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(increase(predictions_generated_total[24h]))",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "short"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 12,
          "y": 0
        }
      },
      {
        "id": 4,
        "title": "Betting Success Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "betting_success_rate",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {
                  "color": "red",
                  "value": 0
                },
                {
                  "color": "yellow",
                  "value": 0.4
                },
                {
                  "color": "green",
                  "value": 0.6
                }
              ]
            },
            "unit": "percentunit"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 18,
          "y": 0
        }
      },
      {
        "id": 5,
        "title": "Model Accuracy Over Time",
        "type": "timeseries",
        "targets": [
          {
            "expr": "model_accuracy",
            "refId": "A",
            "legendFormat": "Accuracy"
          },
          {
            "expr": "model_f1_score",
            "refId": "B",
            "legendFormat": "F1 Score"
          },
          {
            "expr": "model_precision",
            "refId": "C",
            "legendFormat": "Precision"
          },
          {
            "expr": "model_recall",
            "refId": "D",
            "legendFormat": "Recall"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "percentunit",
            "min": 0,
            "max": 1
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 8
        }
      },
      {
        "id": 6,
        "title": "Drift Score Over Time",
        "type": "timeseries",
        "targets": [
          {
            "expr": "drift_score",
            "refId": "A",
            "legendFormat": "Drift Score"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "short",
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": 0
                },
                {
                  "color": "yellow",
                  "value": 0.3
                },
                {
                  "color": "red",
                  "value": 0.5
                }
              ]
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 8
        }
      },
      {
        "id": 7,
        "title": "Prediction Distribution",
        "type": "piechart",
        "targets": [
          {
            "expr": "sum by (prediction) (predictions_by_outcome)",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "short"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 8,
          "x": 0,
          "y": 16
        }
      },
      {
        "id": 8,
        "title": "Betting Performance",
        "type": "timeseries",
        "targets": [
          {
            "expr": "betting_balance",
            "refId": "A",
            "legendFormat": "Balance"
          },
          {
            "expr": "cumulative_profit",
            "refId": "B",
            "legendFormat": "Cumulative Profit"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "currencyGBP"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 8,
          "x": 8,
          "y": 16
        }
      },
      {
        "id": 9,
        "title": "System Health",
        "type": "table",
        "targets": [
          {
            "expr": "up",
            "refId": "A",
            "format": "table"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {
                  "color": "red",
                  "value": 0
                },
                {
                  "color": "green",
                  "value": 1
                }
              ]
            },
            "mappings": [
              {
                "options": {
                  "0": {
                    "text": "Down"
                  },
                  "1": {
                    "text": "Up"
                  }
                },
                "type": "value"
              }
            ]
          }
        },
        "gridPos": {
          "h": 8,
          "w": 8,
          "x": 16,
          "y": 16
        }
      },
      {
        "id": 10,
        "title": "EvidentlyAI Data Quality",
        "type": "timeseries",
        "targets": [
          {
            "expr": "evidently_data_quality_score",
            "refId": "A",
            "legendFormat": "Data Quality"
          },
          {
            "expr": "evidently_missing_values_ratio",
            "refId": "B",
            "legendFormat": "Missing Values %"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "percentunit",
            "min": 0,
            "max": 1
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 24
        }
      },
      {
        "id": 11,
        "title": "Prefect Flow Runs",
        "type": "timeseries",
        "targets": [
          {
            "expr": "prefect_flow_runs_completed",
            "refId": "A",
            "legendFormat": "Completed"
          },
          {
            "expr": "prefect_flow_runs_failed",
            "refId": "B",
            "legendFormat": "Failed"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "short"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 24
        }
      },
      {
        "id": 12,
        "title": "Retraining Events",
        "type": "logs",
        "targets": [
          {
            "expr": "retraining_events",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 32
        }
      }
    ],
    "time": {
      "from": "now-7d",
      "to": "now"
    },
    "refresh": "30s",
    "annotations": {
      "list": [
        {
          "name": "Model Retraining",
          "datasource": "prometheus",
          "enable": true,
          "expr": "model_retrained",
          "iconColor": "red",
          "titleFormat": "Model Retrained"
        },
        {
          "name": "Drift Detection",
          "datasource": "prometheus",
          "enable": true,
          "expr": "drift_detected",
          "iconColor": "yellow",
          "titleFormat": "Drift Detected"
        }
      ]
    },
    "templating": {
      "list": [
        {
          "name": "model_name",
          "type": "query",
          "query": "label_values(model_name)",
          "refresh": 1,
          "includeAll": false,
          "multi": false
        },
        {
          "name": "time_range",
          "type": "interval",
          "query": "1h,6h,12h,1d,7d",
          "refresh": 1,
          "includeAll": false,
          "multi": false
        }
      ]
    }
  },
  "overwrite": true
}
